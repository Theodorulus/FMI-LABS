
@{
    ViewBag.Title = "Index";
}

<h2>News</h2>
<a class="btn btn-primary" href="/Categories/Index">Afisare categorii</a>
<a class="btn btn-primary" href="/Articles/New">Adauga articol</a>
<br />
<br />
<h1>@ViewBag.Message</h1>
<br />
@foreach (Laborator5App.Models.Article article in ViewBag.Articles)
{
    <div class="panel panel-default">
        
        @Html.Partial("ArticleInfo", article)
        <div class="panel-footer">
            <a class="btn btn-sm btn btn-success" href="/Articles/Show/@article.Id">Afisare articol</a>
        </div>
    </div>
}
<a href="/Categories/Index/">Afisare lista categorii</a>
<br />
<a href="/Articles/New">Adauga articol</a>
<br />

